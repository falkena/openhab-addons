{
  "$id": "#State",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Current robot state data",
  "type": "object",
  "properties": {
    "desired": {
      "description": "Unknown. Related to AWS - Cloud",
      "type": "object",
      "properties": {
        "echo": {
          "description": "Unknown. Related to AWS - Cloud",
          "type": [ "boolean", "null" ]
        }
      },
      "required": [ ],
      "extends" : {
        "type" : "object",
        "javaType" : "IRobotDTO"
      }
    },
    "reported": {
      "description": "Current reported robot data",
      "type": "object",
      "properties": {
        "batPct": {
          "description": "Robot battery load",
