          "description": "Allow to upload maps to iRobot claude",
          "type": [ "boolean", "null" ]
        },
        "noAutoPasses": {
          "description": "Write properly help",
          "type": [ "boolean", "null" ]
        },
        "openOnly": {
          "description": "Write properly help",
          "type": [ "boolean", "null" ]
        },
        "pose": {
          "$ref": "Pose.json"
        },
        "signal": {
          "$ref": "Signal.json"
        },
        "twoPass": {
          "description": "Write properly help",
          "type": [ "boolean", "null" ]
        },
        "vacHigh": {
          "description": "Write properly help",
          "type": [ "boolean", "null" ]
        }
      },
      "required": [ ],
      "extends" : {
        "type" : "object",
        "javaType" : "IRobotDTO"
      }
