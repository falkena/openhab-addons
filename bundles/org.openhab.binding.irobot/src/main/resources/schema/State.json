          "description": "Pause vacuum, if bin is full",
          "type": [ "boolean", "null" ]
        },
        "carpetBoost": {
          "description": "Write properly help",
          "type": [ "boolean", "null" ]
        },
        "cleanMissionStatus": {
          "$ref": "CleanMissionStatus.json"
        },
        "cleanSchedule": {
          "$ref": "CleanSchedule.json"
        },
        "lastCommand": {
          "$ref": "LastCommand.json"
        },
