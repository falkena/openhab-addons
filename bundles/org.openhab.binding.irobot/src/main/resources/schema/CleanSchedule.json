{
  "$id": "#CleanSchedule",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$comment": "Roomba 980 schedule description",
  "description": "iRobot schedule package",
  "type": "object",
  "properties": {
    "cycle": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "h": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "m": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    }
  },
  "required": [ "cycle", "h", "m" ],
  "extends" : {
    "type" : "object",
    "javaType" : "IRobotDTO"
  }
}
